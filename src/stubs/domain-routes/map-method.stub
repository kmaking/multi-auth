    /**
     * Define the "{{singularSnake}}" routes for the application.
     *
     * These routes all receive session state, CSRF protection, etc.
     *
     * @return void
     */
    protected function map{{singularClass}}Routes()
    {
        Route::middleware(['web', '{{singularSnake}}', 'auth:{{singularSnake}}'])
            ->prefix('{{singularSlug}}')
            ->domain('{{singularSlug}}.' . env('APP_DOMAIN'))
            ->as('{{singularSlug}}.')
            ->namespace($this->namespace . '\{{singularClass}}')
            ->group(base_path('routes/{{singularSlug}}.php'));
    }

